{
  "name": "Numeric Converter",
  "version": "1.0.0",
  "description": "Plugin to convert hex, binary and decimal numbers inside zazu",
  "blocks": {
    "input": [
      {
        "id": "hex",
        "type": "PrefixScript",
        "prefix": "0x",
        "space": false,
        "args": "Required",
        "script": "src/hex.js",
        "connections": [
          "DisplayResult"
        ]
      },
      {
        "id": "bin",
        "type": "PrefixScript",
        "prefix": "0b",
        "space": false,
        "args": "Required",
        "script": "src/bin.js",
        "connections": [
          "DisplayResult"
        ]
      },
      {
        "id": "template",
        "type": "Keyword",
        "keyword": "template",
        "title": "Zazu Template",
        "subtitle": "Goto Zazu Template GitHub Repo",
        "icon": "fa-files-o",
        "connections": [
          "GoHome"
        ]
      },
      {
        "id": "hello",
        "type": "Keyword",
        "keyword": "hello",
        "title": "Hello!",
        "subtitle": "Please type your name!",
        "icon": "fa-hand-spock-o",
        "connections": ["GoHome"]
      },
      {
        "id": "hello-name",
        "type": "PrefixScript",
        "prefix": "hello",
        "space": true,
        "args": "Required",
        "script": "src/hello.js",
        "connections": ["GoHome"]
      }
    ],
    "output": [
      {
        "id": "GoHome",
        "type": "OpenInBrowser",
        "url": "https://github.com/tinytacoteam/zazu-template"
      },
      {
        "id": "DisplayResult",
        "type": "CopyToClipboard",
        "text": "{value}"
      }
    ]
  }
}
